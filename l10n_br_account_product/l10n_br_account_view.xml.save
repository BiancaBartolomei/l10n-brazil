<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
		<record id="l10n_br_account_product_fiscal_category_form" model="ir.ui.view">
			<field name="name">l10n_br_account_product.fiscal.category</field>
			<field name="model">l10n_br_account.fiscal.category</field>
			<field name="inherit_id" ref="l10n_br_account.view_l10n_br_account_fiscal_category_form"/>
			<field name="arch" type="xml">
				<field name="fiscal_type" position="attributes">
					<attribute name="invisible">0</attribute>
				</field>
			</field>
		</record>	
		<record model="ir.ui.view" id="view_l10n_br_account_mde_form">
	        <field name="name">l10n_br_account_mde.form</field>
	        <field name="model">l10n_br_account.mde</field>
	        <field name="type">form</field>
	        <field name="arch" type="xml">
	        	<form string="l10n_br_account_mde">	
		        	<header>
		        		<button name="action_search_nfe" string="Pesquisar NF-e" type="object"/> 
		        		<button name="action_known_emission" string="Ciência da Emissão" type="object"/> 		
		        		<button name="action_confirm_operation" string="Confirmar Operação" type="object"/> 		
		        		<button name="action_unknown_operation" string="Desconhecer Operação" type="object"/> 		
		        		<button name="action_not_operation" string="Operação não Realizada" type="object"/> 		
		        		<button name="action_download_xml" string="Download do XML" type="object"/> 
	        		</header>		
	        		<sheet>		
	        			<group>	
			                	<field name="chNFe" select="1"/>
			                	<field name="nSeqEvento" select="2"/>
			               		<field name="idLote" select="0"/>
			               		<field name="xNome" select="0"/>
				                <field name="IE" select="0"/>
				                <field name="dEmi" select="0"/>		                
				                <field name="tpNF" select="0"/>
				                <field name="vNF" select="0"/>
			        	        <field name="cSitNFe" select="0"/>
			                	<field name="cSitConf" select="0"/>
				                <field name="formInclusao" select="0"/>
				                <field name="dataInclusao" select="0"/>
				                <field name="versao" select="0"/>               
		        	        </group>
		                </s</form>
	        </field>
	    </record>
	    <record model="ir.ui.view" id="view_l10n_br_account_mde_tree">
	        <field name="name">l10n_br_account_mde.tree</field>
	        <field name="model">l10n_br_account.mde</field>
	        <field name="type">tree</field>
	        <field name="arch" type="xml">
	            <tree string="l10n_br_account_mde" create="false">
	            	<field name="chNFe"/>
	                <field name="nSeqEvento"/>
	                <field name="idLote"/>
	                <field name="xNome"/>
	                <field name="IE"/>
	                <field name="dEmi"/>
	                <field name="tpNF"/>
	                <field name="vNF"/>
	                <field name="cSitNFe"/>
	                <field name="cSitConf"/>
	                <field name="formInclusao"/>
	                <field name="dataInclusao"/>
	                <field name="versao"/>
	            </tree>
	        </field>
	    </record>
	    <record model="ir.actions.act_window" id="action_l10n_br_account_mde">
	        <field name="name">Manifestacao</field>
	        <field name="res_model">l10n_br_account.mde</field>
	        <field name="view_type">form</field>
	        <field name="view_mode">tree,form</field>
	    </record>
	    <menuitem name="Manifestação do Destinatário" id="menu_l10n_br_account_mde" action="action_l10n_br_account_mde" parent="account.menu_finance_payables" sequence="1"/>     
	</data>
</openerp>
